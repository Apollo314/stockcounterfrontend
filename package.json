{
  "name": "stockcounter",
  "version": "0.0.1",
  "description": "An Inventory Management System",
  "productName": "Stock Counter",
  "author": "Apollo314 <abidinsefa@outlook.com>",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "generate-client": "openapi --input http://127.0.0.1:8000/api/schema/ --output ./src/client --useUnionTypes --useOptions --client axios --name AppClient",
    "save-schema": "curl --output ./src/client/schema.json --url http://127.0.0.1:8000/api/schema/?format=json",
    "openapi": "npm run generate-client & npm run save-schema"
  },
  "dependencies": {
    "@quasar/extras": "^1.0.0",
    "@vueuse/core": "^9.13.0",
    "ajv": "^8.12.0",
    "axios": "^1.3.4",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.3",
    "pinia": "^2.0.33",
    "quasar": "^2.11.10",
    "vee-validate": "^4.8.4",
    "vue": "^3.2.47",
    "vue-advanced-cropper": "^2.8.8",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@intlify/vite-plugin-vue-i18n": "^3.3.1",
    "@quasar/app-vite": "^1.0.0",
    "@types/node": "^12.20.21",
    "@types/vue-cropperjs": "^4.1.2",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.0.0",
    "openapi-typescript": "^6.2.1",
    "openapi-typescript-codegen": "^0.23.0",
    "prettier": "2.8.4",
    "typescript": "^5.0.2"
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
